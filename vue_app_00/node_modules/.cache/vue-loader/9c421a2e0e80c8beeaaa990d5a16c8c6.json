件 -->\r\n        <button @click=\"openAlert\">简写提示框</button>\r\n        <hr>\r\n        <button @click=\"openConfirm\">确认提示框</button>\r\n        <hr>\r\n        <button @click=\"openPrompt\">输入确认</button>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    data(){//当前组件共享数据，模块直接读取\r\n        return{}//默认返回一个空对象\r\n    },\r\n    methods:{\r\n        openPrompt(){\r\n            this.$messagebox.prompt(\"请输入年龄\").then((value)=>{//回调函数\r\n                console.log(value)\r\n            }).catch(err=>{console.log(err)});\r\n        },\r\n        openConfirm(){\r\n            //功能:显示确认消息框\r\n            this.$messagebox.confirm(\"是否删除指定数据\")\r\n            .then(action=>{console.log(\"确认\")})//用户选择了确认回到函数\r\n            .catch(err=>{console.log(\"取消\")})//用户选择了取消回调函数\r\n            \r\n        },\r\n        openAlert(){\r\n            //交互提示框\r\n            // 标准方式显示提示框\r\n            //     this.$messagebox({\r\n            //     title:\"我帅吗？\",\r\n            //     message:\"帅！\",\r\n            //     showConfirmButton:true, //是否显示确认按钮\r\n            //     showCancelButton:true //是否显示取消按钮\r\n            // })\r\n            //简写方式\r\n            this.$messagebox(\"我帅吗\",\"帅\")\r\n            },\r\n        openToast(){\r\n            //功能此函数完成显示mint-ui短消息提示框\r\n            //提示框\r\n            this.$toast({\r\n                message:\"操作成功\",//提示文字\r\n                position:\"top\",//提示文字位置顶端\r\n                duration:\"5000\",//提示文字时长\r\n                // 第一个是使用字体的名称 第二个是图标名称(首页)\r\n                iconClass:\"iconfont icon-zhijiayou\"\r\n            })\r\n            // console.log(this)\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n    /* 当前组件专有的样式内容 */\r\n</style>\r\n\r\n"]}]}